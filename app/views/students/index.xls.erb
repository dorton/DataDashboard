<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">Last Name</Data></Cell>
        <Cell><Data ss:Type="String">First Name</Data></Cell>
        <Cell><Data ss:Type="String">Math Bubble</Data></Cell>
        <Cell><Data ss:Type="String">Missed Math AYP</Data></Cell>
        <Cell><Data ss:Type="String">Reading Bubble</Data></Cell>
        <Cell><Data ss:Type="String">Missed Reading AYP</Data></Cell>
        <Cell><Data ss:Type="String">Reading NCE</Data></Cell>
        <Cell><Data ss:Type="String">Reading Raw</Data></Cell>
        <Cell><Data ss:Type="String">Math NCE</Data></Cell>
        <Cell><Data ss:Type="String">Math Raw</Data></Cell>        
      </Row>
    <% @students.each do |student| %>
      <Row>
        <Cell><Data ss:Type="String"><%= student.last_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.first_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.bubble_math %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.met_math_ayp %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.bubble_reading %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.met_reading_ayp %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.reading_nce %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.reading_staar_scale %></Data></Cell> 
        <Cell><Data ss:Type="String"><%= student.math_nce %></Data></Cell> 
        <Cell><Data ss:Type="String"><%= student.math_staar_scale %></Data></Cell>      
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>